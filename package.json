  "author": "SK8S <https://github.com/sk8s-co>",
  "license": "FSL-1.1-ALv2",
  "scripts": {
    "dev": "tsup --watch --onSuccess \"node dist/index.js\"",
    "build": "tsup",
    "start": "yarn build && node dist/index.js",
    "lint": "eslint 'src/**/*.ts' && yarn lint:sh",
    "lint:sh": "shellcheck -s sh src/**/*.sh",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --check \"src/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src",
    "test:integration": "jest --testPathPattern=tests"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.988.0",
    "@peculiar/x509": "^1.14.3",
    "accepts": "^1.3.8",
    "express": "^5.2.1",
    "github-markdown-css": "^5.8.1",
    "handlebars": "^4.7.8",
    "jose": "^6.1.3",
    "js-yaml": "^4.1.1",
    "marked": "^17.0.1",
    "rxjs": "^7.8.2",
    "ua-parser-js": "^2.0.7"
  },
  "devDependencies": {
    "@types/accepts": "^1.3.7",
    "@types/etag": "^1.8.4",
    "@types/express": "^5.0.6",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.6",
    "@types/supertest": "^6.0.3",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "etag": "^1.8.1",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "shellcheck": "^4.1.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
